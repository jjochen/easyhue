FRAMEWORK_SEARCH_PATHS = $(inherited) $(PODS_ROOT)/PhilipsHueSDK-iOS/ "${PODS_ROOT}/PhilipsHueSDK-iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/PhilipsHueSDK-iOS"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/CocoaLumberjack.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/PhilipsHueSDK-iOS"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CocoaLumberjack" -framework "HueSDK_iOS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-EasyHue
PODS_ROOT = ${SRCROOT}/Pods